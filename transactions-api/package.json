{
  "name": "rest-transactions-api",
  "version": "1.0.0",
  "description": "Transactions REST API",
  "scripts": {
    "test": "jest --",
    "testCoverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lintAndTest": "yarn lint && yarn test",
    "start": "tsc && npm run copy-files && npm run copy-config && node dist/src/app.js",
    "copy-files": "copyfiles -u 1 src/**/*.yaml dist/src",
    "copy-config": "copyfiles -u 1 config/*.yaml dist/config/",
    "startDev": "ts-node-dev --inspect=0.0.0.0 ./src/app.ts"
  },
  "keywords": [
    "nodejs",
    "express"
  ],
  "author": "Mart√≠n Pielvitori",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/node": "^18.18.1",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.4.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  },
  "dependencies": {
    "aws-sdk": "^2.1323.0",
    "config": "3.3.1",
    "copyfiles": "^2.4.1",
    "express": "4.17.1",
    "js-yaml": "^4.1.0",
    "swagger-ui-express": "^4.1.6",
    "typescript": "^4.9.4",
    "uuid": "^9.0.0",
    "web3": "^4.1.2",
    "winston": "^3.8.2"
  }
}
